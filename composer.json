{
	"name" : "swango/model",
	"description" : "swango/model is a part of swango framework",
	"license" : "Apache-2.0",
	"keywords" : [
		"php",
		"swoole",
		"orm",
		"polymorphism",
		"singleton"
	],
	"require" : {
		"php" : ">=8.1",
		"swango/cache" : "^1.0",
		"swango/db" : "^1.0",
		"swango/environment" : "^1.0",
		"swango/stdlib" : "^1.2"
	},
	"autoload" : {
		"psr-4" : {
			"Swango\\Model\\" : "src"
		}
	},
	"config" : {
		"optimize-autoloader" : true,
		"sort-packages" : true
	}
}